<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <BuildPackage>true</BuildPackage>
        <PackageId>Maxima.NET.SDK.Integracao</PackageId>
        <Version>1.0.44</Version>
        <Authors>MáximaTech</Authors>
        <Company>MáximaTech</Company>
        <PackageTags>MaximaTech</PackageTags>
        <Copyright>Copyright (c) MáximaTech 2022</Copyright>
        <Description>
      Essa biblioteca tem como objetivo facilitar a integração de dados de qualquer ERP com os sistemas da MáximaTech.
        </Description>
    </PropertyGroup>

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
        <NoWarn>CS1591</NoWarn>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.Hosting" Version="6.0.1" />
        <PackageReference Include="Google.Cloud.PubSub.V1" Version="2.7.0" />
    </ItemGroup>

</Project>
